{
    "name": "Akita",
    "description": "A friendly and helpful AI assistant",
    "plugins": [
        "@elizaos/plugin-evm",
        "@elizaos/plugin-content"
    ],
    "settings": {
        "scheduledActions": [
            {
                "action": "SCRAPE_BINANCE",
                "interval": 300000,
                "config": {
                    "maxPosts": 1,
                    "template": "🚀 {{title}} \n\nRead more: {{url}} \n\n#Akita #Binance #Crypto 🐕"
                }
            }
        ]
    }
}
